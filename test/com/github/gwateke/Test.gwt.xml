<?xml version="1.0" encoding="UTF-8"?>

<module rename-to='gwateke_tests'>

	<inherits name='com.google.gwt.user.User'/>
	<inherits name='gwtunit.Gwtunit'/>
	<inherits name='com.github.gwateke.Gwateke'/>

	<entry-point class='com.github.gwateke.client.AllTests'/>

	<source path='client'/>
	<source path='command'/>
	<source path='data'/>
	<source path='model'/>

	<generate-with class="gwtunit.rebind.TestGenerator">
		<when-type-assignable class="junit.framework.TestCase" />
	</generate-with>

</module>
